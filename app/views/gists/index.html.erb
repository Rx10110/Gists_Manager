<p id="notice"><%= notice %></p>

<h1>Listing Gists</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>ID</th>
      <th>Category</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gists.each do |gist| %>
      <tr>
        <td><%= gist.description %></td>
        <td><%= gist.id %></td>
        <td><%= gist.user_id %></td>
        <td><%= gist.category_id %></td>
        <td><%= gist.url %></td>
        <td><%= link_to 'Show', gist %></td>
        <td><%= link_to 'Edit', edit_gist_path(gist) %></td>
        <td><%= link_to 'Destroy', gist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<br>



<%= link_to 'New Gist', new_gist_path %> </br>
<%= link_to 'Categories', categories_path %> </br>
<%= link_to 'Import', import_form_path %>
